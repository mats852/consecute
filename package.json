{
  "name": "consecute",
  "version": "0.0.0",
  "description": "Handle function execution by pub-sub",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest tests/* --forceExit --maxWorkers=4",
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib/**/*",
    "types.d.ts"
  ],
  "typings": "./types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mats852/consecute.git"
  },
  "keywords": [
    "event-driven",
    "hooks",
    "pub-sub",
    "async"
  ],
  "author": "Mathieu St-Vincent",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mats852/consecute/issues"
  },
  "homepage": "https://github.com/mats852/consecute#readme",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "post-merge": "npm i"
    }
  }
}
